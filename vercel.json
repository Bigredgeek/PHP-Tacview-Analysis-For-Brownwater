{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/debriefing.php",
      "dest": "/api/debriefing.php"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}