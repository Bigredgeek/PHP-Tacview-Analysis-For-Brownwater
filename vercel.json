{
  "version": 2,
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.php"
    },
    {
      "src": "/debriefing.php",
      "dest": "/debriefing.php"
    },
    {
      "src": "/tacview.php",
      "dest": "/tacview.php"
    },
    {
      "src": "/tacview.css",
      "dest": "/tacview.css"
    },
    {
      "src": "/(.*\\.(gif|jpg|jpeg|png|css|js))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "buildCommand": "echo 'PHP project ready for deployment'"
}